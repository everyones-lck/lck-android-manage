<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="20dp"
        >

        <TextView
            android:id="@+id/tv_match_result_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.LCK.Light"
            android:textSize="14sp"
            android:text="LCK Summer Match"
            android:layout_marginStart="48dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>

        <TextView
            android:id="@+id/tv_match_result_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.LCK.Light"
            android:textSize="14sp"
            android:text="2024.01.01"
            android:layout_marginStart="40dp"
            app:layout_constraintStart_toEndOf="@id/tv_match_result_title"
            app:layout_constraintTop_toTopOf="@id/tv_match_result_title"/>

        <TextView
            android:id="@+id/tv_match_result_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.LCK.Light"
            android:textSize="14sp"
            android:text="17:00"
            android:layout_marginStart="5dp"
            app:layout_constraintStart_toEndOf="@id/tv_match_result_date"
            app:layout_constraintTop_toTopOf="@id/tv_match_result_date"/>

        <TextView
            android:id="@+id/tv_match_set1_team"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.LCK.Light"
            android:text="GEN"
            android:layout_marginStart="48dp"
            android:layout_marginTop="20dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_match_result_title"/>

        <TextView
            android:id="@+id/tv_match_divide_set1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.LCK.Light"
            android:text="/"
            android:layout_marginStart="20dp"
            app:layout_constraintStart_toEndOf="@+id/tv_match_set1_team"
            app:layout_constraintTop_toTopOf="@id/tv_match_set1_team"/>

        <TextView
            android:id="@+id/tv_match_set2_team"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.LCK.Light"
            android:text="GEN"
            android:layout_marginStart="20dp"
            app:layout_constraintStart_toEndOf="@id/tv_match_divide_set1"
            app:layout_constraintTop_toTopOf="@+id/tv_match_divide_set1"/>

        <TextView
            android:id="@+id/tv_match_divide_set2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.LCK.Light"
            android:text="/"
            android:layout_marginStart="20dp"
            app:layout_constraintStart_toEndOf="@+id/tv_match_set2_team"
            app:layout_constraintTop_toTopOf="@id/tv_match_set2_team"/>

        <TextView
            android:id="@+id/tv_match_set3_team"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.LCK.Light"
            android:text="GEN"
            android:layout_marginStart="20dp"
            app:layout_constraintStart_toEndOf="@id/tv_match_divide_set2"
            app:layout_constraintTop_toTopOf="@+id/tv_match_divide_set2"/>

        <TextView
            android:id="@+id/tv_match_divide_set3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.LCK.Light"
            android:text="/"
            android:layout_marginStart="20dp"
            app:layout_constraintStart_toEndOf="@+id/tv_match_set3_team"
            app:layout_constraintTop_toTopOf="@id/tv_match_set3_team"/>

        <TextView
            android:id="@+id/tv_match_set4_team"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.LCK.Light"
            android:text="GEN"
            android:layout_marginStart="20dp"
            app:layout_constraintStart_toEndOf="@id/tv_match_divide_set3"
            app:layout_constraintTop_toTopOf="@+id/tv_match_divide_set3"/>

        <TextView
            android:id="@+id/tv_match_divide_set4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.LCK.Light"
            android:text="/"
            android:layout_marginStart="20dp"
            app:layout_constraintStart_toEndOf="@+id/tv_match_set4_team"
            app:layout_constraintTop_toTopOf="@id/tv_match_set4_team"/>

        <TextView
            android:id="@+id/tv_match_set5_team"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            style="@style/TextAppearance.LCK.Light"
            android:text="GEN"
            android:layout_marginStart="20dp"
            app:layout_constraintStart_toEndOf="@id/tv_match_divide_set4"
            app:layout_constraintTop_toTopOf="@+id/tv_match_divide_set4"/>

        <TextView
            android:id="@+id/tv_match_result_divide_lane"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textSize="1dp"
            android:background="#BBBBBB"
            android:alpha="0.4"
            android:layout_marginHorizontal="20dp"
            android:layout_marginTop="20dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_match_set1_team"/>

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>